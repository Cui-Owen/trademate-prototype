{
  "ci": {
    "collect": {
      "staticDistDir": "build",
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.8 }],
        "total-byte-weight": ["error", { "maxNumericValue": 200000 }],
        "interactive": ["error", { "maxNumericValue": 2500 }]
      }
    }
  }
}
